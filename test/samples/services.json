{
  "parameters": {
    "name": "paranoia",
    "setter": "setFoo"
  },

  "services": {
    "foo": {
      "module": "<%= base %>/samples/di_with_parameter_foo",
      "instance": {
        "arguments": [ "Mr.<%= name %>" ]
      }
    },
    "bar": {
      "module": "<%= base %>/samples/di_with_parameter_bar",
      "instance": {
        "arguments": [ ]
      },
      "calls": [
        {
          "method": "<%= setter %>",
          "arguments": [ "@foo" ]
        }
      ]
    }
  }
}